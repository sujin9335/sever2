package com.test;

import java.util.ArrayList;
import java.util.Scanner;

public class Test {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int n=sc.nextInt();
		
		ArrayList<num> list=new ArrayList<num>();
		
		for(int i=0; i<n; i++) {
			num re=new num();
			re.a=sc.nextInt();
			re.b=sc.nextInt();
			re.c=sc.nextInt();
			
			list.add(re);
		}
		
		
		
	}
}

class num {
	
	int a;
	int b;
	int c;
	
	
	
	
	
}
